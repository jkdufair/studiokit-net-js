// Place your settings in this file to overwrite default and user settings.
{
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/lib": true
	},
	"javascript.validate.enable": false,
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": true,
		"source.organizeImports": true
	}
}