{
	"name": "studiokit-net-js",
	"version": "0.0.12",
	"scripts": {
		"build": "babel src -d lib",
		"build:watch": "babel src --watch -d lib",
		"test": "mocha --require babel-polyfill --require isomorphic-fetch --compilers js:babel-core/register 'src/**/__tests__/*'",
		"test:watch": "mocha -w --require babel-polyfill --require isomorphic-fetch --compilers js:babel-core/register 'src/**/__tests__/*'",
		"commit": "commit-wizard"
	},
	"description": "Network/API functions for Studio apps at Purdue",
	"main": "lib/index.js",
	"repository": "purdue-tlt/studiokit-net-js",
	"author": "Jason Dufair <jase@purdue.edu>",
	"license": "MIT",
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"chai": "^3.5.0",
		"eslint": "^3.19.0",
		"eslint-plugin-redux-saga": "^0.3.0",
		"fetch-mock": "^5.10.0",
		"flow-bin": "^0.46.0",
		"immutable": "^3.8.1",
		"isomorphic-fetch": "^2.2.1",
		"mocha": "^3.3.0",
		"pre-git": "^3.14.0",
		"rewire": "^2.5.2"
	},
	"dependencies": {
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.4.0",
		"babel-runtime": "^6.23.0",
		"redux": "^3.6.0",
		"redux-saga": "^0.15.1"
	},
	"config": {
		"pre-git": {
			"commit-msg": "simple",
			"pre-commit": [
				"yarn babel -- src -d lib"
			],
			"pre-push": [],
			"post-commit": [],
			"post-checkout": [],
			"post-merge": []
		}
	}
}
