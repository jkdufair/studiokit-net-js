{
	"name": "studiokit-net-js",
	"version": "1.1.3",
	"scripts": {
		"eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
		"lint": "eslint --ext=js ./*.js src test",
		"fix": "npm run lint -- --fix",
		"test": "cross-env BABEL_ENV=test jest",
		"test:watch": "cross-env BABEL_ENV=test jest --watch",
		"coverage": "cross-env BABEL_ENV=test jest --coverage",
		"coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
		"build": "babel src -d lib",
		"build:watch": "babel src --watch -d lib",
		"precommit": "lint-staged",
		"pretest": "npm run build",
		"prepush": "npm run test",
		"prepublish": "npm run test"
	},
	"description": "Network/API functions for Studio apps at Purdue",
	"main": "lib/index.js",
	"repository": "purdue-tlt/studiokit-net-js",
	"license": "MIT",
	"files": [
		"lib"
	],
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-eslint": "^8.0.0",
		"babel-plugin-rewire": "^1.1.0",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.4.0",
		"babel-runtime": "^6.23.0",
		"coveralls": "^2.13.1",
		"cross-env": "^5.0.5",
		"eslint": "^4.7.2",
		"eslint-config-prettier": "^2.5.0",
		"eslint-config-studiokit": "^0.1.0",
		"eslint-plugin-prettier": "^2.3.1",
		"eslint-plugin-redux-saga": "^0.5.0",
		"fetch-mock": "^5.10.0",
		"flow-bin": "^0.55.0",
		"husky": "^0.14.3",
		"isomorphic-fetch": "^2.2.1",
		"jest": "^21.2.1",
		"lint-staged": "^4.2.2",
		"prettier": "^1.7.0",
		"uuid": "^3.1.0"
	},
	"dependencies": {
		"lodash": "^4.17.4",
		"redux": "^3.6.0",
		"redux-saga": "^0.15.1"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		]
	}
}
