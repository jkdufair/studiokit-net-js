{
	"name": "studiokit-net-js",
	"version": "1.0.5",
	"scripts": {
		"build": "babel src -d lib --ignore '**/*.spec.js'",
		"build:watch": "babel src --watch -d lib --ignore '**/*.spec.js'",
		"test": "BABEL_ENV=test jest",
		"test:watch": "BABEL_ENV=test jest --watch",
		"coverage": "BABEL_ENV=test jest --coverage",
		"coveralls": "BABEL_ENV=test jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"commit": "commit-wizard"
	},
	"description": "Network/API functions for Studio apps at Purdue",
	"main": "lib/index.js",
	"repository": "purdue-tlt/studiokit-net-js",
	"author": "Jason Dufair <jase@purdue.edu>",
	"license": "MIT",
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-plugin-rewire": "^1.1.0",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"coveralls": "^2.13.1",
		"eslint": "^3.19.0",
		"eslint-plugin-redux-saga": "^0.3.0",
		"fetch-mock": "^5.10.0",
		"flow-bin": "^0.46.0",
		"isomorphic-fetch": "^2.2.1",
		"jest": "^20.0.4",
		"pre-git": "^3.14.0"
	},
	"dependencies": {
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.4.0",
		"babel-runtime": "^6.23.0",
		"lodash": "^4.17.4",
		"redux": "^3.6.0",
		"redux-saga": "^0.15.1"
	},
	"config": {
		"pre-git": {
			"commit-msg": "simple",
			"pre-commit": [
				"yarn babel -- src -d lib"
			],
			"pre-push": [],
			"post-commit": [],
			"post-checkout": [],
			"post-merge": []
		}
	},
	"jest": {
		"testPathIgnorePatterns": [
			"/node_modules/",
			"<rootDir>/lib/"
		]
	}
}