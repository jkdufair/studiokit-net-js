{
	"name": "studiokit-net-js",
	"version": "2.0.0",
	"scripts": {
		"eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
		"lint": "eslint --ext=js --ignore-pattern '!.eslintrc.js' src test",
		"fix": "npm run lint -- --fix",
		"test": "cross-env BABEL_ENV=test jest",
		"test:watch": "cross-env BABEL_ENV=test jest --watch",
		"coverage": "cross-env BABEL_ENV=test jest --coverage",
		"coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
		"build": "babel src -d lib",
		"build:watch": "babel src --watch -d lib",
		"prepare": "npm run build"
	},
	"description": "Network/API functions for Studio apps at Purdue",
	"main": "lib/index.js",
	"repository": "purdue-tlt/studiokit-net-js",
	"license": "MIT",
	"files": [
		"lib"
	],
	"devDependencies": {
		"@redux-saga/testing-utils": "^1.0.1",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "9.0.0",
		"babel-plugin-rewire": "^1.2.0",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-runtime": "^6.26.0",
		"babel-template": "^6.26.0",
		"babel-types": "^6.26.0",
		"coveralls": "^3.0.2",
		"cross-env": "^5.2.0",
		"eslint": "5.12.0",
		"eslint-config-prettier": "^4.0.0",
		"eslint-config-studiokit": "^1.0.0",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-redux-saga": "^1.0.0",
		"flow-bin": "^0.92.1",
		"husky": "^1.3.1",
		"jest": "23.6.0",
		"lint-staged": "^8.1.3",
		"prettier": "^1.16.4"
	},
	"dependencies": {
		"lodash": "^4.17.11",
		"redux": "^4.0.1",
		"redux-saga": "^1.0.1",
		"uuid": "^3.3.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run test"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		]
	}
}
